	xsi:schemaLocation="http://www.wabacus.com ../xsd/wabacus.applications.xsd">
	<page id="in" js="/webresources/script/validate.js,/webresources/script/demo.js">
		<header>
			<![CDATA[<p align="center"><font size="6"><b>入库管理</b></font></p>]]>
		</header>
		<report id="userlist" parenttitle="入库管理" type="editablelist" cellresize="1" rowselect="multiple-checkbox" pagesize="13" interceptor="com.wabacusdemo.DeleteUser">
				<display mouseoverbgcolor="CCCCCC" colselect="false">
				<col column="{col-rowselect}" width="20"></col>
					<col column="{sequence:1}" label="序号" width="3%"/>
					<col column="uuid" label="序号" width="5%" clickorderby="true" displaytype="hidden"/>
					<col column="name" label="用户名称" clickorderby="true" width="15%"></col>
					<col column="age" label="角色" clickorderby="true" width="8%"></col>
					<col column="sex" label="备注" width="67%"></col>
				</display>
				<sql>
					<select>
						<value>
							<![CDATA[SELECT uuid,name,age,sex FROM tbl_user order by name]]>
						</value>
					</select>				
				</sql>
